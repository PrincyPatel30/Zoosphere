{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": "off",
    "axe/aria": "off",
    "axe/keyboard": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}